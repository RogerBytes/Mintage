        "my-stations-page": {
            "type": "page",
            "title": "My Stations",
            "sections": [
                "station-list-section"
            ]
        },
        "preferences-page": {
            "type": "page",
            "title": "Preferences",
            "sections": [
                "appearance-section",
                "volume-section",
                "youtube-section"
