            "type": "section",
            "title": "Search Radio station",
            "keys": [
                "radio-search-widget"
            ]
        },
        "appearance-section": {
            "type": "section",
            "title": "Appearance",
            "keys": [
                "icon-type",
                "color-on",
                "color-paused",
                "channel-on-panel"
            ]
        },
        "volume-section": {
            "type": "section",
            "title": "Volume",
            "keys": [
                "keep-volume-between-sessions",
                "initial-volume"
            ]
        },
        "youtube-section": {
            "type": "section",
            "title": "YouTube Download",
            "keys": [
                "youtube-download-cli",
                "music-download-dir-select"
            ]
        }
    },
